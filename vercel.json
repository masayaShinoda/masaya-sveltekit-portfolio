{
    "public": "build",
    "rewrites": [
        { "source": "/(.*)", "destination": "/" },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "courses.masayashida.com"
                }
            ],
            "destination": "https://masayashida.com/courses"
        }
    ]
}